{
    "profile": {
        "id": "INTEGER PRIMARY KEY AUTOINCREMENT",
        "name": "TEXT NOT NULL",
        "ticker": "TEXT UNIQUE NOT NULL",
        "sector": "TEXT",
        "subsector": "TEXT",
        "website": "TEXT",
        "description": "TEXT"
    },
    "quotes": {
        "profile_id": "INTEGER",
        "date": "TEXT NOT NULL",
        "open": "REAL",
        "high": "REAL",
        "low": "REAL",
        "close": "REAL",
        "adj_close": "REAL",
        "volume": "INTEGER",
        "PRIMARY KEY": "(profile_id, date)",
        "FOREIGN KEY": "(profile_id) REFERENCES profile(id) ON DELETE CASCADE"
    },
    "income_statement": {
        "profile_id": "INTEGER",
        "year": "TEXT NOT NULL",
        "total_revenue": "REAL",
        "cost_of_revenue": "REAL",
        "gross_profit": "REAL",
        "operating_expense": "REAL",
        "operating_income": "REAL",
        "pretax_income": "REAL",
        "tax_provision": "REAL",
        "net_income": "REAL",
        "eps": "REAL",
        "total_expenses": "REAL",
        "normalized_income": "REAL",
        "interest_income": "REAL",
        "interest_expense": "REAL",
        "net_interest_income": "REAL",
        "ebit": "REAL",
        "ebitda": "REAL",
        "normalized_ebitda": "REAL",
        "PRIMARY KEY": "(profile_id, year)",
        "FOREIGN KEY": "(profile_id) REFERENCES profile(id) ON DELETE CASCADE"
    },
    "balance_sheet": {
        "profile_id": "INTEGER",
        "year": "TEXT NOT NULL",
        "cash_and_cash_equivalents": "REAL",
        "current_assets": "REAL",
        "goodwill_and_intangible_assets": "REAL",
        "long_term_investments": "REAL",
        "non_current_assets": "REAL",
        "total_assets": "REAL",
        "dividends_payable": "REAL",
        "current_debt": "REAL",
        "current_liabilities": "REAL",
        "long_term_debt": "REAL",
        "non_current_liabilities": "REAL",
        "total_liabilities": "REAL",
        "stockholders_equity": "REAL",
        "total_capitalization": "REAL",
        "tangible_assets": "REAL",
        "working_capital": "REAL",
        "invested_capital": "REAL",
        "book_value": "REAL",
        "total_debt": "REAL",
        "ordinary_shares": "REAL",
        "treasury_shares": "REAL",
        "PRIMARY KEY": "(profile_id, year)",
        "FOREIGN KEY": "(profile_id) REFERENCES profile(id) ON DELETE CASCADE"
    },
    "cash_flow": {
        "profile_id": "INTEGER",
        "year": "TEXT NOT NULL",
        "net_income": "REAL",
        "depreciation_and_amortization": "REAL",
        "working_capital_variation": "REAL",
        "operating_cash_flow": "REAL",
        "investment_cash_flow": "REAL",
        "dividends_paid": "REAL",
        "financing_cash_flow": "REAL",
        "cash_variation": "REAL",
        "initial_cash_position": "REAL",
        "final_cash_position": "REAL",
        "stock_issuance": "REAL",
        "debt_issuance": "REAL",
        "debt_repayment": "REAL",
        "stock_repurchase": "REAL",
        "free_cash_flow": "REAL",
        "PRIMARY KEY": "(profile_id, year)",
        "FOREIGN KEY": "(profile_id) REFERENCES profile(id) ON DELETE CASCADE"
    },
    "stats": {
        "profile_id": "INTEGER",
        "year": "INTEGER NOT NULL",
        "roe": "REAL",
        "payout": "REAL",
        "dividends": "REAL",
        "eps": "REAL",
        "PRIMARY KEY": "(profile_id, year)",
        "FOREIGN KEY": "(profile_id) REFERENCES profile(id) ON DELETE CASCADE"
    }
}